{
	"$id": "https://json-schema.whiteband.ai/judgment/submission/v1.json",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"version": {
			"const": 1
		},
		"uuid": {
			"$ref": "/common/uuid/v7.json"
		},
		"versions": {
			"type": "object",
			"properties": {
				"history": {
					"type": "array",
					"items": {
						"$ref": "/common/version/v1.json"
					}
				},
				"submission": {
					"$ref": "/common/version/v1.json"
				}
			},
			"required": [
				"history",
				"submission"
			]
		},
		"judgment_info": {
			"$ref": "/judgment/content/v1.json"
		}
	},
	"required": [
		"version",
		"uuid",
		"versions",
		"judgment_info"
	]
}